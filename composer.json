{
  "name": "vmware/vcloud-sdk-patched",
  "description": "VMware vCloud SDK for PHP - patched version",
  "type": "library",
  "keywords": [
    "vmware",
    "vcloud",
    "director",
    "api",
    "sdk",
    "patched"
  ],
  "homepage": "https://github.com/singlehopllc/vmware-vcloud-sdk-php-patched",
  "license": "CC-BY-3.0",
  "suggest": {
    "pear-pear/HTTP_Request2": "Requires HTTP_Request2 from pear's repository http://pear.php.net if it is not installed system-wise"
  },
  "autoload": {
    "psr-0": {
      "VMware_": "library/"
    }
  },
  "include-path": [
    "library/"
  ],
  "repositories": [
    {
      "type": "pear",
      "url": "http://pear.php.net"
    }
  ],
  "require": {
    "php": ">=5.3.2"
  },
  "require-dev": {
    "pear-pear/HTTP_Request2": "2.1.*",
    "sami/sami": "1.3.0",
    "vmware/vcloud-sdk": "5.5.0"
  }
}
